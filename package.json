  "build": {
    "extends": null,
    "appId": "com.electron.bahis",
    "publish": [
      {
        "provider": "github",
        "owner": "road86",
        "repo": "bahis-desk",
        "releaseType": "prerelease"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": "always",
      "menuCategory": "bahis",
      "license": "LICENSE",
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "target": [
        "deb",
        "snap"
      ]
    }
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "rm -rf ./dist && yarn react-build && yarn electron-build",
    "release": "yarn react-build && yarn electron-build --p always --win",
    "win-build": "rm -rf ./dist && yarn react-build && yarn electron-build --win",
    "linux-build": "rm -rf ./dist && yarn react-build && yarn electron-build --linux",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "lint-js": "eslint 'src/**/*.{js,jsx}'",
    "lint-ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "electron-rebuild": "electron-rebuild",
    "lint": "eslint */**/*.{js,ts,tsx} --quiet --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
